{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [],
      "outputs": []
    },
    "lint": {},
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "tsc": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}